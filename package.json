{
  "name": "saga-soa",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test": "vitest run",
    "clean": "pnpm -r --if-present clean",
    "dep:list": "pnpm syncpack list",
    "dep:fix": "pnpm syncpack fix",
    "check": "turbo run build --no-cache --force && pnpm test",
    "prepare": "npm run co:login",
    "preinstall": "npm run co:login",
    "co:login": "export CODEARTIFACT_AUTH_TOKEN=$(aws codeartifact get-authorization-token --domain saga --query authorizationToken --output text) && npm config set //saga-531314149529.d.codeartifact.us-west-2.amazonaws.com/npm/saga_js/:_authToken=$CODEARTIFACT_AUTH_TOKEN"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "syncpack": "14.0.0-alpha.16",
    "turbo": "^2.5.4",
    "typescript": "5.8.2",
    "vitest": "^1.5.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "version": "1.0.0",
  "dependencies": {
    "@swc/helpers": "^0.5.17",
    "bunchee": "^6.5.4"
  }
}