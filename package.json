{
  "name": "saga-soa",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "build:clean": "pnpm clean && turbo run build --no-cache --force",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test": "vitest run",
    "clean": "pnpm -r --if-present clean",
    "dep:list": "pnpm syncpack list",
    "dep:fix": "pnpm syncpack fix",
    "check": "turbo run build --no-cache --force && pnpm test",
    "pre-publish": "pnpm build:clean && pnpm test",
    "version:patch": "pnpm -r exec -- pnpm version patch",
    "version:minor": "pnpm -r exec -- pnpm version minor",
    "version:major": "pnpm -r exec -- pnpm version major",
    "publish:dry": "pnpm -r publish --dry-run --access public",
    "publish:packages": "pnpm pre-publish && pnpm -r publish --access public",
    "publish:force": "pnpm -r publish --access public --force",
    "login:github": "npm login --scope=@hipponot --auth-type=legacy --registry=https://npm.pkg.github.com",
    "publish:github": "pnpm pre-publish && pnpm -r publish --registry=https://npm.pkg.github.com --access=public"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "syncpack": "14.0.0-alpha.16",
    "turbo": "^2.5.4",
    "typescript": "5.8.2",
    "vitest": "^1.5.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "version": "1.0.0",
  "dependencies": {
    "@swc/helpers": "^0.5.17",
    "bunchee": "^6.5.4"
  }
}