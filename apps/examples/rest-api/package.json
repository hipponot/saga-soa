{
  "name": "rest-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch --onSuccess \"node dist/main.js\"",
    "build": "tsup",
    "start": "node dist/main.js",
    "check-types": "tsc --noEmit",
    "lint": "echo 'add lint script'",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "prepare": "npm run co:login",
    "preinstall": "npm run co:login",
    "co:login": "export CODEARTIFACT_AUTH_TOKEN=$(aws codeartifact get-authorization-token --domain saga --query authorizationToken --output text) && npm config set //saga-531314149529.d.codeartifact.us-west-2.amazonaws.com/npm/saga_js/:_authToken=$CODEARTIFACT_AUTH_TOKEN"
  },
  "dependencies": {
    "@saga/soa-core-api": "1.0.0",
    "@saga/soa-db": "1.0.0",
    "@saga/soa-logger": "workspace:^",
    "@types/mongodb": "^4.0.7",
    "express": "^4.18.2",
    "figlet": "^1.8.1",
    "inversify": "^6.2.2",
    "mongodb": "^5.7.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.1",
    "tsup": "^8.5.0",
    "vitest": "^1.5.0"
  }
}