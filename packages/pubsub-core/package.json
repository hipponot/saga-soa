{
  "name": "@saga-soa/pubsub-core",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "default": "./dist/adapters/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^4.0.10",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@saga/typescript-config": "workspace:*",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^1.5.0"
  }
} 