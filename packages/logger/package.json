{
  "name": "@saga/soa-logger",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "prepare": "npm run co:login",
    "preinstall": "npm run co:login",
    "co:login": "export CODEARTIFACT_AUTH_TOKEN=$(aws codeartifact get-authorization-token --domain saga --query authorizationToken --output text) && npm config set //saga-531314149529.d.codeartifact.us-west-2.amazonaws.com/npm/saga_js/:_authToken=$CODEARTIFACT_AUTH_TOKEN"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./mocks": {
      "types": "./dist/mocks/mock-logger.d.ts",
      "default": "./dist/mocks/mock-logger.js"
    }
  },
  "dependencies": {
    "inversify": "^6.2.2",
    "pino": "^8.15.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@saga/typescript-config": "workspace:*",
    "@types/node": "latest",
    "pino-pretty": "^10.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module"
}