{
  "name": "@saga/soa-config",
  "version": "1.0.0",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "prepare": "npm run co:login",
    "preinstall": "npm run co:login",
    "co:login": "export CODEARTIFACT_AUTH_TOKEN=$(aws codeartifact get-authorization-token --domain saga --query authorizationToken --output text) && npm config set //saga-531314149529.d.codeartifact.us-west-2.amazonaws.com/npm/saga_js/:_authToken=$CODEARTIFACT_AUTH_TOKEN"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./i-config-manager": {
      "types": "./dist/i-config-manager.d.ts",
      "default": "./dist/i-config-manager.js"
    },
    "./mocks/mock-config-manager": {
      "types": "./dist/mocks/mock-config-manager.d.ts",
      "default": "./dist/mocks/mock-config-manager.js"
    }
  },
  "devDependencies": {
    "@saga/typescript-config": "1.0.0",
    "@types/node": "24.0.12",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "17.2.0",
    "dotenv-flow": "4.1.0",
    "inversify": "^6.2.2",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.2",
    "zod": "3.25.67"
  }
}