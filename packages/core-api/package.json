{
  "name": "@saga-soa/core-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/rest-controller.js",
  "types": "dist/rest-controller.d.ts",
  "exports": {
    "./rest-controller": {
      "types": "./dist/rest-controller.d.ts",
      "default": "./dist/rest-controller.js"
    },
    "./express-server": {
      "types": "./dist/express-server.d.ts",
      "default": "./dist/express-server.js"
    },
    "./express-server-schema": {
      "types": "./dist/express-server-schema.d.ts",
      "default": "./dist/express-server-schema.js"
    }
  },
  "scripts": {
    "dev": "bunchee --watch",
    "build": "tsup",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@saga-soa/config": "workspace:*",
    "@saga-soa/logger": "workspace:*",
    "express": "^4.18.2",
    "figlet": "^1.8.1",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.14",
    "routing-controllers": "^0.11.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.5.3",
    "@types/node": "latest",
    "bunchee": "^6.5.4",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "typesVersions": {
    "*": {
      "rest-controller": ["dist/rest-controller.d.ts"]
    }
  }
}